    "cross-env": "^7.0.3",
    "gulp-nodemon": "^2.5.0",
    "js-comments": "^0.5.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.4",
    "yargs": "^17.0.1"
  }
