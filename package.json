    "lint": "eslint --fix {server,test,config}/**/*.{js,ts}",
    "prettier": "prettier",
    "prettier:base": "npm run prettier **/*.{js,json,scss,md,ts,html,component.html}",
    "prettier:write": "npm run prettier:base --write",
    "prettier:check": "npm run prettier:base --list-different",
    "route:list": "node fs route:list",
    "route:cache": "node fs route:cache",
    "make:controller": "node fs make:controller",
    "make:model": "node fs make:model",
    "make:schema": "node fs make:schema",
    "make:policy": "node fs make:policy"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bee-queue": "^1.4.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.18.1",
    "fastify-cors": "^6.0.1",
    "fastify-static": "^4.2.2",
    "mongoose": "^5.13.0",
    "nanoid": "^3.1.23",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.6.2",
    "pino": "^6.11.3",
    "pino-pretty": "^5.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^15.12.5",
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "^6.3.8",
