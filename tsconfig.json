{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "removeComments": true,
    "lib": ["ES2020"],
    "allowJs": true,
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "pretty": true,
    "paths": {
      "@models/*":["./app/models/*"]
    }
  },
  "exclude": ["node_modules", "**/*.spec.ts", "src/core/internal","*.*","dist"]
}